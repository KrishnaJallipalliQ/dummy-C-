	"scripts": {
		"lint": "prettier-eslint --write \"api/**/*.js\"",
		"inspect": "node --inspect app.js",
		"inspect-ms1": "node --inspect=9231 ./micro-services/refreshOverviewCache/app.js",
		"start": "node_modules/pm2/bin/pm2 start ecosystem.json && node_modules/pm2/bin/pm2 logs",
		"start-clear": "pm2 kill && rm -rf ~/.pm2/logs/*.* && rm -rf ./logs/*.* && node_modules/pm2/bin/pm2 start ecosystem.config.js && node_modules/pm2/bin/pm2 logs",
		"start-online": "node_modules/pm2/bin/pm2 start ecosystem-online.json && node_modules/pm2/bin/pm2 logs",
		"start-offline": "node_modules/pm2/bin/pm2 start ecosystem-offline.json && node_modules/pm2/bin/pm2 logs",
		"start-windows": "pm2-dev start ecosystem.json && pm2 logs",
		"test": "node --inspect $NODE_DEBUG_OPTION test.js",
		"test-coverage": "istanbul cover test.js && istanbul report clover",
		"test-windows": "node --inspect test.js",
		"api-test": "node --inspect $NODE_DEBUG_OPTION api-test.js",
		"api-coverage": "istanbul cover api-test.js && istanbul report clover",
		"api-windows": "node --inspect api-test.js",
		"logs": "pm2 logs",
		"delete": "pm2 delete all",
		"deletewindows": "taskkill /F /IM node.exe",
		"webdriver": "webdriver-manager start",
		"e2e": "webdriver-manager start --standalone & protractor e2e/config/e2e.config.js",
		"slack": "node slack.js",
		"updateGeoDb": "node updateGeoDb.js",
		"pm2-update": "pm2 update",
		"di": "node  --max-old-space-size=8192 --inspect=7777 tasks/holdingsDataIntegrity.js",
		"preinstall": "npx npm-force-resolutions",
		"migrate:up": "npx sequelize-cli db:migrate"
	},
	"resolutions": {
		"graceful-fs": "^4.2.10"
	},
